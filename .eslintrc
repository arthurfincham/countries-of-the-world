{
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "root": true,
  "plugins": ["import", "prettier", "react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-eval": "error",
    "import/first": "error",
    "react/prop-types": "off",
    "no-inline-comments": "error"
  },

  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
